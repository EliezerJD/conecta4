<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane  fx:id="root" pickOnBounds="false" prefHeight="220.0" prefWidth="300.0" styleClass="bodybg" stylesheets="@../media/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.MainController">
   <children>
      <ImageView fx:id="conectaLogo" fitHeight="150.0" fitWidth="200.0" layoutX="50.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../media/conecta-logo.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnPlay" fitHeight="150.0" fitWidth="200.0" layoutX="50.0" layoutY="76.0" onMouseClicked="#play" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../media/play-logo.png" />
         </image>
      </ImageView>
      <ImageView fx:id="loadingGif" fitHeight="100.0" fitWidth="100.0" layoutX="100.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../media/loading3.gif" />
         </image>
      </ImageView>
      <ImageView fx:id="error" fitHeight="172.0" fitWidth="225.0" layoutX="40.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../media/error.png" />
         </image>
      </ImageView>
      <Button fx:id="btnAtras" focusTraversable="false" layoutX="123.0" layoutY="185.0" mnemonicParsing="false" onAction="#volver" text="Volver" visible="false" />
   </children>
</AnchorPane>
